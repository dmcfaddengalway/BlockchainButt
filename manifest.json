{
  "name": "Blockchain Butt",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Replaces the blockchain buzzword with buttcrack.",
  "icons": {
    "16": "icons/block-butt-16.png",
    "48": "icons/block-butt-48.png",
    "128": "icons/block-butt-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/background.js"
      ],
      "run_at": "document_end"
    }
  ]
}
